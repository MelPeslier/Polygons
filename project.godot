;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Polygons"
config/tags=PackedStringArray("experiment", "learning")
config/features=PackedStringArray("4.2", "Mobile")

[debug]

gdscript/warnings/native_method_override=1

[editor_plugins]

enabled=PackedStringArray("res://addons/custom_polygon_tool/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
